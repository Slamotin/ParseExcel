<Window x:Class="ParseExcel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ParseExcel"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel Margin="10" >
        <WrapPanel DockPanel.Dock="Top" Height="30" HorizontalAlignment="Right">
            <Button Name="buttonStart" HorizontalAlignment="Right" Height="20" 
                    Content="Start" Width="60" Click="buttonStart_Click"/>
        </WrapPanel>


        <ListBox Name="listBox1" Margin="0,0,150,0" Width="600" SelectionChanged="listBox1_SelectionChanged">

        </ListBox>
        <Button Name="filterInXlsxFile" Content="filterInXlsxFile" 
                DockPanel.Dock="Top" HorizontalAlignment="Right" Height="20" Margin="-76,200,0,0" 
                Click="filterInXlsxFile_Click"/>
        <Rectangle Name="rect_Status" Height="20" VerticalAlignment="Top" Width="40" Margin="-56,26,0,-0.5" Fill="Red"/>
        <Button Content="JoinXlsX" Height="20" VerticalAlignment="Top" Width="67" Margin="-46,60,0,0" Click="Button_Click"/>


    </DockPanel>
</Window>
